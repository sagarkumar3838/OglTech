{
  "name": "skill-evaluation-platform",
  "version": "2.0.0",
  "description": "AI-powered technical skill evaluation platform with RAG",
  "scripts": {
    "verify": "npx tsx verify-setup.ts",
    "install:all": "npm install && cd client && npm install && cd ../server && npm install",
    "dev:client": "cd client && npm run dev",
    "dev:server": "cd server && set NODE_OPTIONS=--no-deprecation && npm run dev",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "start": "npm run dev",
    "build:client": "cd client && npm run build",
    "build:server": "cd server && npm run build",
    "build": "npm run build:client && npm run build:server",
    "deploy": "npm run build && firebase deploy",
    "type-check": "cd client && npm run type-check && cd ../server && npm run type-check",
    "setup": "npm run install:all",
    "seed:questions": "npx tsx scripts/seed-question-bank.ts",
    "seed:careers": "cd server && npm run seed:ogl-careers",
    "seed:careers-client": "npx tsx scripts/seed-careers-client.ts",
    "upload-csv-options": "npx tsx scripts/upload-csv-with-options.ts"
  },
  "keywords": [
    "evaluation",
    "assessment",
    "typescript",
    "firebase",
    "rag",
    "ai"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/papaparse": "^5.5.2",
    "concurrently": "^8.2.2",
    "dotenv": "^16.6.1",
    "tsx": "^4.21.0"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@lineiconshq/free-icons": "^1.0.4",
    "@lineiconshq/react-lineicons": "^1.0.5",
    "@supabase/supabase-js": "^2.95.3",
    "assemblyai": "^4.22.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "csv-parse": "^6.1.0",
    "devicon": "^2.17.0",
    "firebase": "^12.8.0",
    "framer-motion": "^12.29.0",
    "framework7": "^9.0.2",
    "gsap": "^3.14.2",
    "mongodb": "^7.0.0",
    "mongoose": "^9.1.5",
    "openai": "^6.16.0",
    "papaparse": "^5.5.3",
    "tailwind-merge": "^3.4.0"
  }
}
